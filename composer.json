{
    "name": "marcodelriego/ml-cms",
    "description": "A PHP usecase: CMS",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Relmaur",
            "email": "marck94zac1@gmail.com"
        }
    ],
    "scripts": {
        "make:migration": "php bin/make-migration.php",
        "make:controller": "php bin/make-controller.php",
        "make:model": "php bin/make-model.php",
        "migrate:rollback": "php bin/migrate-rollback.php",
        "migrate": "php bin/migrate.php",
        "db:backup": "php bin/backup-database.php",

        "test": "./vendor/bin/phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3"
    },
    "require": {
        "symfony/mailer": "^7.3",
        "symfony/dotenv": "^7.3"
    }
}
